{
  "name": "mkg-app",
  "version": "1.0.0",
  "description": "MKG管理アプリ",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "dev:browser": "scripts\\dev-with-browser.bat",
    "dev:stable": "next dev --port 3000",
    "dev:codespaces": "next dev --hostname 0.0.0.0 --port 3000",
    "dev:clean": "npm run clean && npm run dev:stable",
    "build": "next build",
    "start": "next start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf .next",
    "process:manage": "dev-process-manager.bat",
    "process:kill": "taskkill /f /im node.exe",
    "process:check": "netstat -ano | findstr :300"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shunsukekanou/MKG-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shunsukekanou/MKG-app/issues"
  },
  "homepage": "https://github.com/shunsukekanou/MKG-app#readme",
  "dependencies": {
    "@supabase/supabase-js": "^2.56.1",
    "dotenv": "^17.2.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "next": "^14.2.4",
    "next-pwa": "^5.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "rimraf": "^6.0.1"
  }
}
